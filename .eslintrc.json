{
    "extends": [
        "airbnb",
        "plugin:flowtype/recommended"
    ],
    "plugins": [
        "flowtype",
        "compat"
    ],
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "compat/compat": 2
    },
    "env": {
        "jest": true
    }
}
